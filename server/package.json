{
  "name": "nodejsexpressapp",
  "version": "1.0.0",
  "description": "Start building your next Node.js Express app on IBM Cloud.",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "dev": "nodemon server/server.js",
    "start:dev": "tsnd --transpile-only --respawn --ignore-watch node_modules server/server.ts",
    "prestart": "npm run build",
    "start": "npm run serve",
    "check-coverage": "nyc report --check-coverage --lines 100",
    "lint": "eslint . --ext .ts",
    "fix": "eslint --ignore-path .gitignore . --fix",
    "test": "nyc mocha test --recursive --exit",
    "build": "tsc",
    "serve": "node dist/server/server"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "exclude": [
      "test"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "dependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "ibm-cloud-env": "^0",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.4.0",
    "eslint-config-strongloop": "^2.1.0",
    "husky": "^4.2.5",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.56"
  }
}
